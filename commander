#renew the case
cd github/CS-sqlite
git pull


#modify and hand in
cd github/CS-sqlite
git add --all .
git commit -m 'description about this commit'
git push origin master 

#return to pre version
	1 show log
#git log              (q to return)
	2 return pre version
#git reset --hard HEAD^    (pre second ==>HEAD^^ pre 100 ==>HEAD~100)
