#clone the case
mkdir github
cd github
git clone https://github.com/Liangwz/CS-sqlite.git

#renew the case
cd github/CS-sqlite
git pull


#modify and hand in
cd github/CS-sqlite
git add --all .
git commit -m 'description about this commit'
git push origin master 

#return to pre version
1 show log
#git log              (q to return)
2 return pre version
#git reset --hard HEAD^    (pre second ==>HEAD^^ pre 100 ==>HEAD~100)
sumary:
To where#git reset --hard commit_id
To past#git log

To future#git reflog




#rm file
rm text.tx
==>#1 recover 
git checkout -- text.tx
==>#2 remove
git rm text.tx
##git status
git commit -m 'remove file'
git push origin master
